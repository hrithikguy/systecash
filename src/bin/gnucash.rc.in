    BEGIN
      VALUE "CompanyName", "systecash Development Team"
      VALUE "FileDescription", "systecash Program File"
      VALUE "FileVersion", "@VERSION@"
      VALUE "InternalName", "systecash"
      VALUE "LegalCopyright", "Â©2011 systecash Development Team, Licence: GPL v2.0 or later"
      VALUE "OriginalFilename", "systecash.exe"
      VALUE "ProductName", "systecash Free Finance Manager"
      VALUE "ProductVersion", "@VERSION@"
    END
  END

  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x409, 1252
  END
